    <style name="applied_row_list">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="RadioButtonStyle" parent="@android:style/Widget.CompoundButton">
        <item name="android:button">@null</item>
    </style>

    <style name="more_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:typeface">serif</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="KO_Daw_Layout">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@null</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="employer_Tab_Txt_Style">
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorPrimary">@color/greendark</item>
        <item name="colorPrimaryDark">@color/colorAccent</item>

    </style>



    <style name="cv_regist">
        <item name="android:background">@drawable/regist_frame</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_toRightOf">@id/no_icon</item>
        <item name="android:layout_margin">5dp</item>
    </style>


    <style name="job_detail_show">
        <item name="android:gravity">center</item>
        <item name="android:textColor">#000</item>
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_alignLeft">@+id/empOpenJobLogo</item>
    </style>

    <style name="Job_Detail_WorkCode">
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textColor">#000</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_alignLeft">@+id/empJobCategory</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxLines">2</item>
        <item name="android:layout_width">wrap_content</item>
    </style>




</resources>


